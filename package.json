{
  "name": "react-aurora",
  "version": "0.0.1",
  "description": "Draw your popups, modals, context menus, tooltips in document 'atmosphere'",
  "main": "./lib/react-aurora.js",
  "scripts": {
    "build": "rollup -c",
    "test": "jest ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monar/react-aurora.git"
  },
  "keywords": [
    "popup",
    "modal",
    "windows",
    "absolute",
    "context",
    "menu",
    "tooltip",
    "react",
    "manager"
  ],
  "author": "Piotr Tomasz Monarski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Monar/react-aurora/issues"
  },
  "homepage": "https://github.com/Monar/react-aurora#readme",
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-preset-react": "21.2.0",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2"
  },
  "jest": {
    "setupFiles": [
      "./testPolyfills.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost"
  },
  "babel": {
    "presets": [
      "react"
    ]
  }
}
