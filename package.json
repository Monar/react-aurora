{
  "name": "react-aurora",
  "version": "0.0.11",
  "description": "Draw your popups, modals, context menus, tooltips in document 'atmosphere'",
  "main": "./lib/react-aurora.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monar/react-aurora.git"
  },
  "keywords": [
    "popup",
    "modal",
    "windows",
    "absolute",
    "context",
    "menu",
    "tooltip",
    "react",
    "manager"
  ],
  "author": "Piotr Tomasz Monarski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Monar/react-aurora/issues"
  },
  "homepage": "https://github.com/Monar/react-aurora#readme",
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-react": "6.24.1",
    "eslint": "4.16.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-class-property": "1.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.5.0",
    "eslint-plugin-react": "7.6.0",
    "prettier": "1.10.2",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "rollup": "0.55.1",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-node-resolve": "3.0.2"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "external-helpers"
    ]
  }
}
