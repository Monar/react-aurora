{
  "name": "react-aurora",
  "version": "0.1.1",
  "description": "Draw your popups, modals, context menus, tooltips in document 'atmosphere'",
  "main": "lib/react-aurora.js",
  "main": "lib/react-aurora.cjs.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --fix .",
    "test": "jest",
    "build:es5": "rollup -c ./rollup.config/es5.js",
    "build:es6": "rollup -c ./rollup.config/es6.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monar/react-aurora.git"
  },
  "keywords": [
    "popup",
    "modal",
    "windows",
    "absolute",
    "context",
    "menu",
    "tooltip",
    "react",
    "manager"
  ],
  "author": "Piotr Tomasz Monarski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Monar/react-aurora/issues"
  },
  "homepage": "https://github.com/Monar/react-aurora#readme",
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "rollup": "0.66.4",
    "rollup-plugin-babel": "4.0.3",
    "babel-jest": "23.6.0",
    "babel-core": "7.0.0-bridge.0",
    "jest": "23.6.0",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-test-renderer": "16.5.2",
    "regenerator-runtime": "0.12.1",
    "rollup-plugin-node-resolve": "3.4.0"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "env": {
      "test": {
        "presets": [ [ "env" ], "react" ]
      }
    },
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }
}
